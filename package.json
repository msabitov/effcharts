{
  "name": "effcharts",
  "version": "1.1.0",
  "description": "Self-confident 2D charts",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "keywords": [
    "pie chart",
    "donut chart",
    "line chart",
    "area chart",
    "bar chart",
    "stacked bar chart",
    "web components",
    "ui",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "./bar": {
      "types": "./dist/types/bar.d.ts",
      "import": "./dist/bar.js",
      "require": "./dist/bar.js"
    },
    "./line": {
      "types": "./dist/types/line.d.ts",
      "import": "./dist/line.js",
      "require": "./dist/line.js"
    },
    "./pie": {
      "types": "./dist/types/pie.d.ts",
      "import": "./dist/pie.js",
      "require": "./dist/pie.js"
    }
  },
  "author": "Marat Sabitov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "playwright": "^1.50.1",
    "rollup": "^4.30.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://sourcecraft.dev/msabitov/effcharts"
  },
  "homepage": "https://effnd.tech/charts/"
}
